{
  "version": 2,
  "builds": [
    {
      "src": "frontend/view/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/frontend/view/index.html",
      "dest2": "/frontend/view/index.css"
    },
    {
      "src": "/mangas(.*)",
      "dest": "/frontend/view/mangas.html",
      "dest2": "/frontend/view/index.html"
    },
    {
      "src": "/filmes(.*)",
      "dest": "/frontend/view/filmes.html",
      "dest2": "/frontend/view/index.html"
    }
  ]
}