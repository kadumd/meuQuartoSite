{
  "version": 2,
  "builds": [
    {
      "src": "API/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "frontend/view/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/API/index.js"
    },
    {
      "src": "/mangas",
      "dest": "/frontend/view/$1"
    }
  ]
}
