{
  "version": 2,
  "builds": [
    {
      "src": "frontend/view/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/frontend/view/index.html"
    },
    {
      "src": "/css/index.css",
      "dest": "/frontend/view/css/index.css"
    },
    {
      "src": "/mangas",
      "dest": "/frontend/view/mangas.html"
    },
    {
      "src": "/filmes",
      "dest": "/frontend/view/filmes.html"
    }
  ]
}